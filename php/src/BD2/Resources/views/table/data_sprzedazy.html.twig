{% extends 'table-layout.html.twig' %}

{% block tableHeader %}
    <th><abbr title="INTEGER">id</abbr></th>
    <th><abbr title="INTEGER">miesiac_id</abbr></th>
    <th><abbr title="INTEGER">numer_miesiac</abbr></th>
    <th><abbr title="INTEGER">numer_tydzien</abbr></th>
    <th><abbr title="VARCHAR2(50)">nazwa</abbr></th>
    <th><abbr title="NUMBER(1)">czy_dzien_roboczy</abbr></th>
{% endblock %}

{% block tableContent %}
    <td><b>{{ item.ID }}</b></td>
    <td>{{ item.MIESIAC_ID }}</td>
    <td>{{ item.NUMER_MIESIAC }}</td>
    <td>{{ item.NUMER_TYDZIEN }}</td>
    <td>{{ item.NAZWA }}</td>
    <td>{{ item.CZY_DZIEN_ROBOCZY }}</td>
{% endblock %}
