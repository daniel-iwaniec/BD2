{% extends 'table-layout.html.twig' %}

{% block tableHeader %}
    <th><abbr title="INTEGER">{{ knp_pagination_sortable(pagination, 'id', 'ID', [], direction) }}</abbr></th>
    <th><abbr title="INTEGER">{{ knp_pagination_sortable(pagination, 'stanowisko_id', 'STANOWISKO_ID') }}</abbr></th>
    <th><abbr title="VARCHAR2(200)">{{ knp_pagination_sortable(pagination, 'imie', 'IMIE') }}</abbr></th>
    <th><abbr title="VARCHAR2(200)">{{ knp_pagination_sortable(pagination, 'nazwisko', 'NAZWISKO') }}</abbr></th>
{% endblock %}

{% block tableContent %}
    <td><b>{{ item.ID }}</b></td>
    <td>{{ item.STANOWISKO_ID }}</td>
    <td>{{ item.IMIE }}</td>
    <td>{{ item.NAZWISKO }}</td>
{% endblock %}
